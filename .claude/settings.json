{
  "projectName": "Pryceless Solutions",
  "permissions": {
    "allow": [
      "Read(*)",
      "Edit(*)",
      "Write(src/**)",
      "Write(public/**)",
      "Write(.claude/**)",
      "Bash(npm:*)",
      "Bash(npx:*)",
      "Bash(git:*)",
      "Bash(node:*)",
